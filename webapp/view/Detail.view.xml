<mvc:View xmlns="sap.uxap" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:m="sap.m"
	xmlns:t="sap.ui.table" xmlns:goals="sap.uxap.sample.SharedBlocks.goals" xmlns:personal="sap.uxap.sample.SharedBlocks.personal"
	controllerName="hts.controller.Detail" height="100%" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:f="sap.ui.layout.form">
	<m:Page title="{i18n>appTitle}" showNavButton="true" navButtonPress="onNavBack">
		<m:headerContent>
			<m:Text text="{i18n>Version}"/>
		</m:headerContent>
		<m:content>
			<m:VBox>
				<ObjectPageLayout id="ObjectPageLayout">
					<headerTitle>
						<ObjectPageHeader id="headerForTest" objectTitle="Tariff Confermation Status - {local>/Detail/LowerAgeText}" showTitleSelector="false"
							showMarkers="true" objectImageShape="Circle"></ObjectPageHeader>
					</headerTitle>
					<headerContent>
						<layout:VerticalLayout>
							<m:ObjectStatus title="Vender Name" text="{local>/Detail/VendorName}"/>
							<m:ObjectStatus title="Vender Address"
								text="{local>/Detail/To_Vendor/VndrAddr1},{local>/Detail/To_Vendor/VndrCity},{local>/Detail/To_Vendor/VndrZipCd}"/>
						</layout:VerticalLayout>
						<!--<m:ProgressIndicator percentValue="30" displayValue="30%" showValue="true" state="None"/>-->
						<layout:VerticalLayout>
							<m:ObjectStatus title="Product Import Description" text="{local>/Detail/InvoiceDesc}"/>
							<m:ObjectStatus title="Product Country Of Origin" text="{local>/Detail/To_Country/MktCntryDesc}"/>
							<!--<m:Label text="{i18n>DocTotal}"/>-->
							<!--<m:ObjectNumber number="{DocTotal}" numberUnit="EUR"/>-->
						</layout:VerticalLayout>
						<!--<m:ObjectAttribute title="{i18n>Status}" text="In Stock"/>-->
						<!--<m:RatingIndicator maxValue="8" class="sapUiSmallMarginBottom" value="4" tooltip="Rating Tooltip"/>-->
					</headerContent>
				</ObjectPageLayout>
				<m:HBox class="sapUiTinyMarginTop">
					<m:VBox class="sapUiSmallMarginEnd">
						<m:Title text="Description/Comment"/>
						<m:Input value="{local>/Detail/Description1}" editable="false" width="100%"/>
						<m:Input value="{local>/Detail/Description2}" editable="false" width="100%"/>
					</m:VBox>
					<m:VBox class="sapUiSmallMarginEnd">
						<m:Title text="%" class="sapUiSmallMarginBegin"/>
						<m:Input value="{local>/Detail/PercentUse}" editable="true" width="100%"/>
					</m:VBox>
					<m:VBox class="sapUiSmallMarginEnd">
						<m:Title text="Tariff Code Alt tariff" class="sapUiLargeMarginBegin"/>
						<m:Select items="{path: '/MatCodeSet',parameters:{expand : 'To_TrfCode'}}" selectedKey="">
							<core:Item key="{To_TrfCode/TrfCdNm}" text="{To_TrfCode/TrfCdDesc}"/>
						</m:Select>
					</m:VBox>
					<m:VBox class="sapUiSmallMarginEnd">
						<m:Title text="Duty/Alt Rate"/>
						<m:Text text="{local>/Detail/To_TrfCode/DutyRate}"></m:Text>
					</m:VBox>
					<m:VBox class="sapUiSmallMarginEnd">
						<m:Title text="Country Of Origin"/>
						<m:Text text="{local>/Detail/To_Country/MktCntryDesc}"/>
					</m:VBox>
					<m:VBox class="sapUiSmallMarginEnd">
						<m:Title text="Other Govt. Agcy/Code FDA Num"/>
						<m:Select items="{path: '/MatCodeSet',parameters:{expand : 'To_Agency'}}" selectedKey="">
							<core:Item key="{To_Agency/OtherGovtAgcyId}" text="{To_Agency/OtherGovtAgcyDesc}"/>
						</m:Select>
					</m:VBox>
					<m:VBox class="sapUiSmallMarginEnd">
						<m:Title text="Vendor"/>
						<m:Select items="{path: '/MatCodeSet'}" selectedKey="{/local/Detail/VendorName}">
							<core:Item key="{VendorName}" text="{VendorName}"/>
						</m:Select>
					</m:VBox>
				</m:HBox>
			</m:VBox>
		</m:content>
		<!--<m:footer>-->
		<!--	<m:Toolbar>-->
		<!--		<m:ToolbarSpacer/>-->
		<!--		<m:Button id="idonSave" type="Emphasized" text="{i18n>Save}" press="onSave" width="5cm" enabled="false"></m:Button>-->
		<!--	</m:Toolbar>-->
		<!--</m:footer>-->
	</m:Page>
</mvc:View>