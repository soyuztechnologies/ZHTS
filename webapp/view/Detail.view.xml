							<m:VBox >
								<!--<m:Title text="%" class="sapUiSmallMarginBegin"/>-->
								<m:Input value="{local>PercentUse}" editable="true" width=""/>
							</m:VBox>
							<m:VBox >
								<!--<m:Title text="Tariff Code Alt tariff"/>-->
