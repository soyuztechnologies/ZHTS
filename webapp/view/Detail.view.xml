<mvc:View xmlns="sap.uxap" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:m="sap.m"
	xmlns:t="sap.ui.table" xmlns:goals="sap.uxap.sample.SharedBlocks.goals" xmlns:personal="sap.uxap.sample.SharedBlocks.personal"
	controllerName="hts.controller.Detail" height="100%" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:f="sap.ui.layout.form">
	<m:Page title="{i18n>appTitle}" showNavButton="true" navButtonPress="onNavBack">
		<m:headerContent>
			<m:Text text="{i18n>Version}"/>
		</m:headerContent>
		<m:content>
			<m:VBox>
			<ObjectPageLayout id="ObjectPageLayout">
				<headerTitle>
					<ObjectPageHeader id="headerForTest" objectTitle="Tariff Confermation Status - {local>/Detail/0/To_TrfCnfSt/TrfCnfrmnStatusDesc}" showTitleSelector="false"
						showMarkers="true" objectImageShape="Circle"></ObjectPageHeader>
				</headerTitle>
				<headerContent>
					<layout:VerticalLayout>
						<m:ObjectStatus title="Vender Name" text="{local>/Detail/0/VendorName}"/>
						<m:ObjectStatus title="Vender Address"
							text="{local>/Detail/0/To_Vendor/VndrAddr1},{local>/Detail/0/To_Vendor/VndrCity},{local>/Detail/0/To_Vendor/VndrZipCd}"/>
					</layout:VerticalLayout>
					<layout:VerticalLayout>
						<m:ObjectStatus title="Product Import Description" text="{local>/Detail/0/InvoiceDesc}"/>
						<m:ObjectStatus title="Product Country Of Origin" text="{local>/Detail/0/To_Country/MktCntryDesc}"/>
					</layout:VerticalLayout>
				</headerContent>
			</ObjectPageLayout>
			<m:Table items="{path:'local>/Detail'}">
				<!--<m:infoToolbar>-->
				<!--	<m:OverflowToolbar>-->
				<!--		<m:Label text="Material"/>-->
				<!--	</m:OverflowToolbar>-->
				<!--</m:infoToolbar>-->
				<m:columns>
					<m:Column>
						<m:header>
						<m:Label text="Description"></m:Label>
						</m:header>
					</m:Column>
					<m:Column>
						<m:header>
						<m:Label text="%"></m:Label>
						</m:header>
					</m:Column>
					<m:Column >
						<m:header>
						<m:Label text="Tariff"></m:Label>
						</m:header>
					</m:Column>
					<m:Column minScreenWidth="Tablet" demandPopin="true">
						<m:header>
						<m:Label text="Duty"></m:Label>
						</m:header>
					</m:Column>
					<m:Column>
						<m:header>
						<m:Label text="Country"></m:Label>
						</m:header>
					</m:Column>
					<m:Column>
						<m:header>
						<m:Label text="other"></m:Label>
						</m:header>
					</m:Column>
					<m:Column>
						<m:header>
						<m:Label text="Vendor"/>
						</m:header>
					</m:Column>
				</m:columns>
				<m:items>
					<m:ColumnListItem>
						<m:cells>
							<m:VBox >
								<m:Input value="{path:'local>Description1'}" editable="false" />
								<m:Input value="{local>Description2}" editable="false" width=""/>
							</m:VBox>
							<m:VBox >
								<m:Input value="{local>PercentUse}" editable="true" width=""/>
							</m:VBox>
							<m:VBox >
								<m:Select items="{path: '/TrfCodeSet'}" selectedKey="{local>To_TrfCode/TrfCdId}">
									<core:Item key="{TrfCdId}" text="{TrfCdDesc}"/>
								</m:Select>
							</m:VBox>
							<m:VBox >
								<m:Input value="{local>To_TrfCode/DutyRate}" editable="false" />
							</m:VBox>
							<m:VBox >
								<m:Input value="{local>To_Country/MktCntryDesc}" editable="false" />
							</m:VBox>
							<m:VBox >
								<m:Select items="{path: '/GovtAgcySet'}" selectedKey="{local>To_Agency/OtherGovtAgcyId}">
									<core:Item key="{OtherGovtAgcyId}" text="{OtherGovtAgcyDesc}"/>
								</m:Select>
							</m:VBox>
							<m:VBox >
								<m:Select items="{path: '/VendorSet'}" selectedKey="{local>To_Vendor/VndrId}">
									<core:Item key="{VndrId}" text="{VndrNm}"/>
								</m:Select>
							</m:VBox>
						</m:cells>
					</m:ColumnListItem>
				</m:items>
			</m:Table>
			</m:VBox>
		</m:content>
	</m:Page>
</mvc:View>